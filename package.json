{
  "name": "get-modules-graph",
  "version": "0.0.3",
  "description": "Get and traverse graph of ECMAScript/TypeScript modules",
  "author": "uid11",
  "bugs": "https://github.com/uid11/get-modules-graph/issues",
  "dependencies": {
    "parse-imports-exports": "0.0.5"
  },
  "devDependencies": {
    "@types/node": ">=20",
    "prettier": ">=3",
    "typescript": ">=5.1"
  },
  "exports": {
    ".": "./index.js"
  },
  "files": [
    "*.js",
    "index.d.ts",
    "types.d.ts"
  ],
  "homepage": "https://github.com/uid11/get-modules-graph#readme",
  "keywords": [
    "ecmascript",
    "esm",
    "graph",
    "modules",
    "traverse",
    "typescript"
  ],
  "license": "MIT",
  "packageManager": "npm@9",
  "repository": {
    "type": "git",
    "url": "https://github.com/uid11/get-modules-graph"
  },
  "scripts": {
    "prebuild": "rm -f *.js *.d.ts",
    "build": "tsc",
    "prettier": "prettier --cache --cache-strategy=content --write . !package-lock.json",
    "test": "export _START=$(date +%s%3N) && npm run prettier && npm run build && node ./index.spec.js",
    "prepublishOnly": "npm test && rm index.spec.js types.js"
  },
  "sideEffects": false,
  "types": "index.d.ts"
}
